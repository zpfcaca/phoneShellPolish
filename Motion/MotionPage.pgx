<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="MotionPage">
    <Code><![CDATA[begin
  cls()
  bExit=false
  do
    call RefreshPage()
    call RefreshBelt()
    delay(0)
  until bExit
  bExit=false
end]]></Code>
